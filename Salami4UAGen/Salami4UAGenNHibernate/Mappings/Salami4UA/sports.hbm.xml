<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="Salami4UAGenNHibernate"
		                   namespace="Salami4UAGenNHibernate.EN.Salami4UA">
			<class name="SportsEN" table="[Sports]">
						<id name="Name" column="[Name]"/>
			
			
		  			
						<bag name="User" table="[sports_user]"  >
							<key>
									<column name="[FK_Name_idSports]"/>
							</key>
      						<many-to-many class="Salami4UAGenNHibernate.EN.Salami4UA.UserEN">
									<column name="[FK_Nickname_idUser]"/>
      						</many-to-many>
    					</bag>
					
					
		
		  </class>
		  
		  <query name="SportsENReadAllHQL">from SportsEN</query>
		  <query name="SportsENdameTodosLosDeportesHQL"><![CDATA[FROM SportsEN]]></query>
		</hibernate-mapping>
	